<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.db.demo01.Mapper.SpotMapper">
   <select id="getSpotByName" resultType="Spot">
       select * from traveldb.t_spot where SpotName = #{SpotName}
   </select>

    <select id="getAllSpot" resultType="Spot">
        select * from traveldb.t_spot;
    </select>

</mapper>